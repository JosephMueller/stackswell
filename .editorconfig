# EditorConfig is awesome:
# http://EditorConfig.org

root = true

[**]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = tab
continuation_indent_size = 2
trim_trailing_whitespace = false
insert_final_newline = true
quote_type = single

[*.{py,json}]
indent_size = 4
indent_style = space

[*.{html,json}]
quote_type = double
